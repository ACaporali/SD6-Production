{% extends "SD6ProductionAppBundle:Include:layout.html.twig" %}

{% block title %}Editer - {{ parent() }}{% endblock %}

{% block head %}
  {{ parent() }}
  <meta name="description" content="">
  <meta name="keywords" content="">
{% endblock %}

{% block site_body %}
	<div class="bordures">
	  	{{ parent() }}
        {% if advert is defined %}
    	  	<h1>Editer {{advert.title}}</h1>
            <div class="form-annonce">
                {{ include("SD6ProductionAdminBundle:Include:form-annonce.html.twig") }}
            </div>
        {% endif %}
        {% if image is defined %}
    	  	<h1>Editer image n°{{image.id}}</h1>
            <div class="form-annonce">
                {{ include("SD6ProductionAdminBundle:Include:form-image.html.twig") }}
            </div>
        {% endif %}
        {% if member is defined %}
    	  	<h1>Editer membre n°{{member.id}}</h1>
            <div class="form-annonce">
                {{ include("SD6ProductionAdminBundle:Include:form-membre.html.twig") }}
            </div>
        {% endif %}
        {% if category is defined %}
    	  	<h1>Editer categorie n°{{category.id}}</h1>
            <div class="form-annonce">
                {{ include("SD6ProductionAdminBundle:Include:form-categorie.html.twig") }}
            </div>
        {% endif %}
	</div>

{% endblock %}
